{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "target": "es5",
        "lib": [
            "es5",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "allowJs": true,
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
}